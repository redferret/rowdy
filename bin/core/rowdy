// Non Native functions

func forEachOn(list, f) {
	loop x: {
		elem = $get(list, x as int)
		breakOut = $f($get(list, x as int))
		x = x + 1
		if x == $size(list) or breakOut {break}
	}
	if breakOut {
		return elem
	}
}
