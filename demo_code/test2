

func main() {

	executeAll = func(map, len, callFunctionAs) {
		loop x: {
			f = get(map, x);
			->callFunctionAs(f);
			if (x >= len - 1){
				break;
			} else {
				x = x+1;
			}
		}
	}
	a1 = array(
		func(){
			print "First Function\n";
		}, 
		func(){
			print "Second Function\n";
		}
	);
	
	a2 = array(
		func(p1){
			if (isset p1) {
				print "Hello, ", p1, "!\n";
			} else {
				print "Hello Stranger\n";
			}
		}, 
		func(){
			print "I'm Rowdy\n";
		}
	);
	
	f1 = func(f) {->f()}// Pass no arguments all all functions
	f2 = func(f) {->f("Richard")} // Pass "Richard" to all functions
	
	->executeAll(a1, 2, f1)
	->executeAll(a2, 2, f2)
	
}