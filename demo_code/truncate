
	func main(var1){
		if (isset var1){
			print ->truncate(var1), "\n";
		} else {
			return 1;
		}
	}

	func truncate(variable) {
		originalValue = variable;
		variable = round variable, 0;
		if (variable < originalValue){
			return variable;
		} else {
			difference = variable - originalValue;
			deciPart = 1 - difference;
			return originalValue - deciPart;
		}
	}
