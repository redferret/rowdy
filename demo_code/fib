
	import "core.rowdy"
	
	func main(max) {
		time = $runtime(func() {
			if (isset max) == false {
				max = 1000
			}
			$println($fibby(max as long))
		})
		
		$println(concat "Took ", time, " ms to finish")
	}

	func fibby(max) {
		fibs = $List()
		x = 0 as long
		z = 0 as long
		y = 1 as long
		loop fib: {
			$add(fibs, x)
			z = x + y
			x = y
			y = z
			if x >= max {
				break
			}
		}
		return fibs
	}