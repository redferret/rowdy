
	import "core.rowdy"
	
	func main(max) {
		time = $runtime(func() {
			if (isset max) == false {
				max = 1000
			}
			$println($fibby(max))
		})
		
		$println(concat "Took ", time, " ms to finish")
	}

	func fibby(max) {
		fibs = $List()
		z = 0
		y = 1
		$forLoop(0, max, 1, func(x) {
			$add(fibs, x as long)
			z = x + y
			x = y
			y = z
		})
		return fibs
	}