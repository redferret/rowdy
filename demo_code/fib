
	import "rowdy.core"
	
	func main(max) {
		time = $runtime(func() {
			max = is max? 1000
			print $getFibList(max as long), ln
		})
		
		print "Took ", time, " ms to finish", ln
	}

	func getFibList(max) {
		fibs = new []
		x = 0 as long
		z = 0 as long
		y = 1 as long
		while (x <= max) {
			$add(fibs, x)
			z = x + y
			x = y
			y = z
		}
		$add(fibs, x)
		return fibs
	}