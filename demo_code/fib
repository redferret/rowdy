
	import "core.rowdy"
	
	func main(max) {
		if (isset max) == false {
			max = 1000
		}
		y = 1
		loop x: { if x >= max {break}
			print x, "\n"
			z = x + y
			x = y
			y = z
		}
		
		list = array(7, 5, 3, 8)
		print list, "\n"
		
		squareAll = func(list) {
			sqrs = $List()
			$forEach(list, func(item){
				v = item ^ 2
				$add(sqrs, int v)
			})
			return sqrs
		}
		sqrs = $squareAll(list)
		$println(sqrs)
	}
