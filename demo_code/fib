
	import "core.rowdy"
	
	func main(max) {
		time = $runtime(func() {
			if $not(isset max){
				max = 1000
			}
			print $fibby(max as long), ln
		})
		
		print "Took ", time, " ms to finish", ln
	}

	func fibby(max) {
		fibs = $List()
		x = 0 as long
		z = 0 as long
		y = 1 as long
		loop fib: {
			$add(fibs, x)
			z = x + y
			x = y
			y = z
			if x >= max {
				$add(fibs, x)
				break
			}
		}
		return fibs
	}