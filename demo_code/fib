
	import "core.rowdy"
	
	func main(max) {
		if (isset max) == false {
			max = 1000
		}
		y = 1
		loop x: { if x >= max {break}
			print x, "\n"
			z = x + y
			x = y
			y = z
		}
		map = $createMap()
		$println(map)
		$put(map, "key", func() {
			return 10
		})
		$println(map)
		$println($get(map, "key"))
		f = $get(map, "key")
		$println(concat $f(), " was returned")
	}
