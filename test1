
	// THIS FILE WILL NOT RUN PROPERLY
	// This file is used for planning and working out new features

	func main() {
		loop x in range 0, 10, 1 { // x is between -1 and 11 incremented by 1
			// Loop goes for 10 iterations
			break x; // break out of this loop
			break; // Same as line above
		}
		// LOOP_STMT => LOOP ID STMT_BLOCK
		// LOOP_STMT => LOOP ID IN RANGE_EXPR STMT_BLOCK
		// RANGE_EXPR => RANGE ARITHM_EXPR , ARITHM_EXPR , ARITHM_EXPR
		
		array1 = array(9, 1, 2, 3, 4);
		array1 = array(9, array(), 2, 3, 4);
		// EXPRESSION => ARRAY ( ARRAY_BODY )
		// ARRAY_BODY => ARRAY_LINEAR
		// ARRAY_LINEAR => ARRAY_VALUE
		
		array2 = array(5:7, "hello":1);
		// EXPRESSION => ARRAY ( ARRAY_BODY )
		// ARRAY_BODY => ARRAY_KEY_VALUE
		// ARRAY_KEY_VALUE => ARRAY_PAIR ARRAY_PAIR_TAIL;
		// ARRAY_PAIR => ARRAY_KEY : ARRAY_VALUE
		// ARRAY_PAIR_TAIL => , ARRAY_PAIR ARRAY_PAIR_TAIL
		// ARRAY_VALUE => EXPRESSION
		
		var1 = array2["hello"] // assigns 1
		// EXPRESSION => ID ARRAY_ACCESS  
		// ARRAY_ACCESS => LBRACKET ARRAY_KEY RBRACKET
		// ARRAY_KEY => EXPRESSION
		
		array3 = range 0, 20, 5; // array(0, 5, 10, 15, 20)
		
	}
