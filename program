
	// Demo to show how to setup callbacks

	func callbackFunc(msg) {
	  print "Callback: ", msg, "\n";
	}
	
	func main() {
		
		->service(callbackFunc);
		
		
		->doSomething((response){
			// On Success
			print response, "\n";
		}, (response){
			// On Failure
			print response, "\n";
		});
	}
	
	
	
	func service(callback) {
	  ->callback("Hello World!");
	}
	
	func doSomething(success, failure) {
		if someConditionIsTrue then
			->success();
		else
			->failure();
		fi
	}
