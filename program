
	// Demo to show how to setup callbacks

	func f1(msg)
		print "f1: ", msg, "\n";
	.
	func service1()
		return 0;
	.
	func service2()
		return 1;
	.
	
	func sendRequest(callback, service)
		opt = ->service();
		if opt == 1 then
			->callback("Request sent Successfully");
		else
			->callback("Request Failed");
		fi
	. 

	func main()
		->sendRequest(f1, service1);
		->sendRequest(f1, service2);
	.
	
.